    "build": {
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "e2e": {
      "cache": true
    }
  },
  "installation": {
    "version": "20.3.2",
    "plugins": {
      "@nx/gradle": "20.3.2",
      "@nx/workspace": "20.3.2"
    }
  },
  "plugins": [
    {
      "plugin": "@nx/gradle",
      "options": {
        "testTargetName": "test",
        "classesTargetName": "classes",
        "buildTargetName": "build",
        "ciTargetName": "test-ci"
      }
