  "nxCloudUrl": "https://staging.nx.app",
  "nxCloudId": "679415331b1f888450ce7a79",
  "namedInputs": {
    "default": ["{projectRoot}/**/*"],
    "production": ["default", "!{projectRoot}/src/test/**/*"]
